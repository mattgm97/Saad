{
  "flex-layout.col#contact-container": {
    "children": [
      "flex-layout.row#contact-title",
      "vtex.menu@2.x:menu#contact-footer-desktop-1"
    ],
    "props": {
      "blockClass": "linkList-container",
      "preventHorizontalStretch": true,
      "fullWidth": true
    }
  },
  "flex-layout.row#contact-title": {
    "children": ["rich-text#contact-title-0"]
  },

  "rich-text#contact-title-0": {
    "props": {
      "font": "t-heading-5",
      "textPosition": "LEFT",
      "textAlign": "LEFT",
      "blockClass": "footer-sobre",
      "text": "Informações"
    }
  },

  "vtex.menu@2.x:menu#contact-footer-desktop-1": {
