          "iconId": null,
          "highlight": false,
          "itemProps": {
            "type": "internal",
            "href": "/nos",
            "noFollow": false,
            "tagTitle": "Nós",
            "text": "Nós"
          }
        },
        {
          "id": "footer-item-acc-1",
          "blockClass": "footer-sobre__menu-item",
